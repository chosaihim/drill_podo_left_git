 <!-- velodyne VLP16 lidar -->
  <joint name="Head_velodyne_joint" type="fixed">
    <axis xyz="0 0 1" />
    <origin xyz="0 0 0.036" rpy="0 0 0"/>
    <parent link="Body_vision"/>
    <child link="Head_velodyne_lidar"/>
  </joint>
  
  <link name="Head_velodyne_lidar">    
  </link>
 
  <gazebo reference="Head_velodyne_lidar">
    <sensor type="ray" name="VLP16">
	<pose frame="">0 0 0 0 0 0</pose>
		
      <visualize>false</visualize>
      <update_rate>10</update_rate>
      <ray>
        <scan>
          <horizontal>
                <samples>440</samples>
                <resolution>1</resolution>
                <min_angle>-3.14</min_angle>
                <max_angle>3.14</max_angle>
              </horizontal>
              <vertical>
                <samples>16</samples>
                <resolution>1</resolution>
                <min_angle>-15.0*3.14/180.0</min_angle>
                <max_angle> 15.0*3.14/180.0</max_angle>
              </vertical>
        </scan>
        <range>
          <min>0.50</min>
          <max>50.0</max>
          <resolution>0.01</resolution>
        </range>
        <noise>
          <type>gaussian</type>
          
          <mean>0.0</mean>
          <stddev>0.01</stddev>
        </noise>
      </ray>

      <!-- must install by sudo apt get ros-kinetic-velodyne-gazeboplugin -->

      <plugin name="laser_controller" filename="libgazebo_ros_velodyne_laser.so">
        <topicName>/mobile_hubo/lidar/velodyne_points</topicName>
        <frameName>Head_velodyne_lidar</frameName>
        <alwaysOn>true</alwaysOn>
      <updateRate>0</updateRate>
      </plugin>
    </sensor>
   </gazebo>

   
  <!-- 3DM-GX5 imu -->
  <!-- 3DM-GX5 imu pose needs revision -->
  <joint name="Head_imu_joint" type="fixed">
    <axis xyz="0 0 1" />
    <origin xyz="0 0 -0.01" rpy="0 0 0"/>
    <parent link="Body_vision"/>
    <child link="Head_imu"/>
  </joint>
  
  <link name="Head_imu">  
  </link>
  
  <gazebo reference="Head_imu">
     <gravity>true</gravity>
    <sensor name="imu_sensor" type="imu">
      <always_on>true</always_on>
      <update_rate>100</update_rate>
      <visualize>true</visualize>
      <topic>__default_topic__</topic>
      <plugin filename="libgazebo_ros_imu_sensor.so" name="imu_plugin">
        <topicName>/mobile_hubo/imu</topicName>
        <bodyName>Head_imu</bodyName>
        <updateRateHZ>10.0</updateRateHZ>
        <gaussianNoise>0.0</gaussianNoise>
        <xyzOffset>0 0 0</xyzOffset>
        <rpyOffset>0 0 0</rpyOffset>
        <frameName>imu_link</frameName>
      </plugin>
      <pose>0 0 0 0 0 0</pose>
    </sensor>
  </gazebo>


  
